 {{> header}}
{{# each foods}}
<div class="w3l_banner_nav_right">
	{{# each this}}
	<div class="col-sm-6 col-md-4">
		<div class="thumbnail">
		  <a href="/single/{{this._id}}">> <img src="{{this.img}}" alt="..."> </a>
		  <div class="caption">
			<h3>{{this.title}}</h3>
			<p>
				<div class="description">
					{{this.description}}
				</div>
			</p>
			<div class="clearfix">
			<div class="price pull-left">Price: ${{this.price}}</div>
			<div>
				<a href="/add-to-cart/{{ this._id }}" class="btn btn-success pull-right">Add to Cart</a>
			</div>
			</div>
			
		  </div>
		</div>
	</div>
	{{/each}}
</div>
{{/each}}

<ul class="pagination pagination-lg">
						<li class=""><a href="/product/1">1</a></li>
